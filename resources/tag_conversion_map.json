{
  "morphology_mappings": {
    "1sg": ["first_person", "singular"],
    "2sg": ["second_person", "singular"],
    "3sg": ["third_person", "singular"],
    "1pl": ["first_person", "plural"],
    "2pl": ["second_person", "plural"],
    "3pl": ["third_person", "plural"],
    "acc": ["accusative"],
    "gen": ["genitive"],
    "dat": ["dative"],
    "nom": ["nominative"],
    "voc": ["vocative"],
    "imp-sg": ["imperative", "singular"],
    "imp-pl": ["imperative", "plural"],
    "inf": ["infinitive"],
    "masc": ["masculine"],
    "fem": ["feminine"],
    "neut": ["neuter"]
  },
  "compound_tag_patterns": {
    "^adverb-(.+)$": ["adverb"],
    "^prep-(.+)$": ["preposition"],
    "^preposition_(.+)$": ["preposition"],
    "^verb_(.+)$": ["verb"],
    "^conjunction_(.+)$": ["conjunction"],
    "^particle_(.+)$": ["particle"],
    "^particles_(.+)$": ["particle"],
    "^adjective_(.+)$": ["adjective"],
    "^noun_(.+)$": ["noun"],
    "^subject_(.+)$": [],
    "^object_(.+)$": [],
    "^article_(.+)$": ["article"],
    "^genitive_(.+)$": ["genitive"],
    "^dative_(.+)$": ["dative"],
    "^accusative_(.+)$": ["accusative"],
    "^nominative_(.+)$": ["nominative"],
    "^vocative_(.+)$": ["vocative"],
    "^copula_(.+)$": ["verb"],
    "^imperative$": ["imperative"],
    "^aorist_(.+)$": ["aorist"],
    "^aspect_(.+)$": [],
    "^idiom-(.+)$": [],
    "^deictic$": [],
    "^negation_(.+)$": [],
    "^prohibition_(.+)$": ["imperative"],
    "^negative_command$": ["imperative"],
    "^contrast_(.+)$": [],
    "^impersonal(.*)$": [],
    "^infinitive$": ["infinitive"],
    "^agreement$": [],
    "^acc_plural$": ["accusative", "plural"],
    "^men_de$": ["particle"],
    "^combined$": []
  },
  "simple_tag_mappings": {
    "pron": ["pronoun"],
    "εἰμί": ["verb", "irregular"],
    "δυνατός": ["adjective"],
    "reading": [],
    "passage": [],
    "vocative": ["vocative"],
    "ouketi": [],
    "deuro": [],
    "molis": [],
    "idou": [],
    "epei": ["conjunction"],
    "kai": ["conjunction"],
    "te": ["particle"],
    "de": ["particle"],
    "gar": ["particle"],
    "oun": ["particle"]
  },
  "chapter_handling": {
    "extract_patterns": [
      "^ch(\\d+)$",
      "^athenaze1$",
      "^athenaze_b1_ch(\\d+)$",
      "^wb(\\d+[a-z])$"
    ],
    "default_source": "athenaze"
  }
}
