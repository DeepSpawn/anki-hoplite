    "delete",
    "old",
    "new",
    "test"
  ],
  "case_sensitive": false,
  "normalize_tags": true,
  "auto_tag_rules": [
    {
      "name": "masculine_article",
      "pattern": "^ο$|^του$|^τω$|^τον$|^οι$|^των$|^τοις$|^τους$",
      "tags": ["article", "masculine"],
      "match_field": "front"
    },
    {
      "name": "feminine_article",
      "pattern": "^η$|^της$|^τη$|^την$|^αι$|^ταις$|^τας$",
      "tags": ["article", "feminine"],
      "match_field": "front"
    },
    {
      "name": "neuter_article",
      "pattern": "^το$|^τα$",
      "tags": ["article", "neuter"],
      "match_field": "front"
    },
    {
      "name": "aorist_augment_sigma",
      "pattern": "^ε[^\\s]+σα",
      "tags": ["aorist"],
      "match_field": "front"
    }
  ]
}
